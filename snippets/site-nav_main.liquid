<style>
  header .main-menu .main-nav-list li.has-big-nav .big-nav .two-column-product-section ul li.featured-card .inner-wrapper video,
  header .main-menu .main-nav-list li.has-big-nav .big-nav .two-column-product-section ul li.featured-card .inner-wrapper iframe {
    width: calc(100% + 2px);
  }
  header .main-menu .main-nav-list li.has-big-nav .big-nav .two-column-product-section {
    border-radius: 0;
  }
  header .main-menu .main-nav-list li.has-big-nav .big-nav .two-column-product-section ul li.featured-card .inner-wrapper {
    padding: 0 30px;
    justify-content: center;
  }
  header .main-menu .main-nav-list li.has-big-nav .big-nav .two-column-product-section ul li.featured-card .content {
    margin-bottom: -10px;
  }
</style>

<ul class="flex-justify-center main-nav-list desktop-menu-main">
  {% for link in mainMenu.links %}
    {% if link.links != blank %}
      <!-- 1If first child menus are there -->
      {% assign parent_index = forloop.index %}
      {% assign linkTitle = link.title | downcase | remove: ' ' %}


        <li class="{% if linkTitle contains 'practitioner' %}link-practitioner{% endif %} {% if link.levels ==  2 %}has-big-nav{%else%}has-sub-nav{%endif%}" aria-haspopup="true">
        {% comment %} href=" {{ link.url}}" {% endcomment %}


        <a
          href=" {{ link.url}}"
          title=" {{ link.title | escape }}"
          class="site-nav__link none-980"
          data-meganav-type="parent"
          aria-controls="MenuParent-{{ parent_index }}"
          aria-expanded="false"
          {% unless request.page_type == 'index' %}
          {%- if link.active %}
          aria-current="page"
          {% endif -%}
          {% endunless %}
          itemprop="item">
          {{ link.title | escape }}
        </a>
        <p
          class="site-nav__link accordion-toggle block-980"
          data-meganav-type="parent"
          tabindex="0"
          itemprop="name"
          {% unless request.page_type == 'index' %}
          {%- if link.active %}
          aria-current="page"
          {% endif -%}
          {% endunless %}>
          {{ link.title | escape }}
        </p>
        <meta itemprop="position" content="1">
        {% comment %} BIG NAV {% endcomment %}


        {% if link.levels == 2 %}
          <div class="big-nav accordion-content">
            <div class="container-1360">
              <div class="flex">
                {% if linkTitle contains 'products' %}
                  <div class="big-nav-menu flex product-icon-links">
                    {% for childlink in link.links %}
                      {% if childlink.links != blank %}
                        {% assign childlinkTitle = childlink.title | downcase | remove: ' ' %}


                        <div class="big-nav-links icon-link">
                          <a
                            href="{{childlink.url}}"
                            title=" {{ childlink.title | escape }}"
                            class="none-980 sub-link pre-title border"
                            aria-controls="MenuParent-{{ child_index }}"
                            aria-expanded="false"
                            itemprop="item">
                            <div>{{ childlink.title }}</div>
                          </a>
                          <p class="accordion-toggle-inner block-980" itemprop="name">
                            {{ childlink.title | escape }}
                          </p>
                          <meta itemprop="position" content="2">


                          <ul class="accordion-content-inner product-inner">
                            {% for subChildlink in childlink.links %}
                              {% assign subChildlinkTitle = subChildlink.title | downcase | remove: ' ' %}
                              <li class="flex">
                                {% assign _object = subChildlink.object %}


                                {% if childlinkTitle contains 'shopbyhealth' %}
                                  {% if _object.metafields.menu.icons != blank %}
                                    {% assign pageImage = _object.metafields.menu.icons | first %}
                                    <img
                                      class="lazyload"
                                      src="{{ pageImage.cloudinary_src}}"
                                      class="none-980">
                                  {% endif %}
                                  <a
                                    href="{{ _object.url }}"
                                    data-meganav-type="parent"
                                    aria-controls="MenuParent-{{ parent_index }}"
                                    aria-expanded="false"
                                    {% unless request.page_type == 'index' %}
                                    {%- if subChildlink.active %}
                                    aria-current="page"
                                    {% endif -%}
                                    {% endunless %}
                                    itemprop="item">
                                    <div itemprop="name">
                                      {{ subChildlink.title | escape }}
                                    </div>
                                  </a>
                                  <meta itemprop="position" content="3">
                                {% else %}
                                  <a
                                    href="{{ subChildlink.url }}"
                                    data-meganav-type="parent"
                                    aria-controls="MenuParent-{{ parent_index }}"
                                    aria-expanded="false"
                                    {% unless request.page_type == 'index' %}
                                    {%- if subChildlink.active %}
                                    aria-current="page"
                                    {% endif -%}
                                    {% endunless %}
                                    itemprop="item">
                                    <div itemprop="name">
                                      {{ subChildlink.title | escape }}
                                    </div>
                                  </a>
                                  <meta itemprop="position" content="3">
                                {% endif %}
                              </li>
                            {% endfor %}
                          </ul>
                        </div>
                      {% endif %}
                    {% endfor %}
                  </div>
                {% else %}
                  <div class="big-nav-menu flex">
                    {% for childlink in link.links %}
                      {% if childlink.links != blank %}
                        {% assign childlinkTitle = childlink.title | downcase | remove: ' ' %}


                        <div class="big-nav-links">
                          <a
                            href="{{childlink.url}}"
                            title=" {{ childlink.title | escape }}"
                            class="none-980 sub-link pre-title border"
                            aria-controls="MenuParent-{{ child_index }}"
                            aria-expanded="false">
                            {{ childlink.title }}
                          </a>
                          <p class="accordion-toggle-inner block-980">
                            {{ childlink.title | escape }}
                          </p>
                          <ul class="accordion-content-inner">
                            {% for subChildlink in childlink.links %}
                              <li>
                                <a
                                  href="{{ subChildlink.url }}"
                                  data-meganav-type="parent"
                                  aria-controls="MenuParent-{{ parent_index }}"
                                  aria-expanded="false"
                                  {% unless request.page_type == 'index' %}
                                  {%- if subChildlink.active %}
                                  aria-current="page"
                                  {% endif -%}
                                  {% endunless %}>
                                  {{ subChildlink.title | escape }}
                                </a>
                              </li>
                            {% endfor %}
                          </ul>
                        </div>
                      {% endif %}
                    {% endfor %}
                  </div>
                {% endif %}


                <div class="two-column-product-section none-980 {% if linkTitle contains 'products' %} one-column-product{%endif%}">
                  <ul class="flex-wrap">
                    {% for block in section.blocks -%}
                      {%- case block.type -%}
                        {%- when 'block' -%}
                          {% assign linkTitlecat = block.settings.category | downcase | remove: ' ' %}
                          {% if linkTitle contains linkTitlecat %}
                            {% if block.settings.title != blank %}
                              {% if block.settings.contentColor != blank %}
                                <style>
                                  .featured-card.white .content * {
                                    color: {{ block.settings.contentColor }}
                                     !important;
                                  }
                                </style>
                              {% endif %}
                              <li class="
                                    featured-card {% if block.settings.content_alignment == "center" %} center-align{%endif%} {% if block.settings.content_alignment == "right" %} right-align{%endif%}{% if
                                    block.settings.content_alignment == "left" %} left-align{%endif%}  {% if block.settings.contentColor != blank %} white{% endif %}
                                  ">
                                <a
                                  href="{{block.settings.link}}"
                                  title="{{block.settings.linkText}}"
                                  class="inner-wrapper">
                                  {% if block.settings.video != blank %}
                                    {% if block.settings.video contains '.mp4' %}
                                      <video
                                        muted
                                        autoplay
                                        playsinline>
                                        <source src="{{ block.settings.video }}" type="video/mp4">
                                      </video>
                                    {% else %}
                                      {% assign videoId = block.settings.video | split: 'vimeo.com/' %}
                                      <iframe
                                        src="https://player.vimeo.com/video/{{ videoId[1] }}?autoplay=1&muted=1"
                                        frameborder="0"
                                        allow="fullscreen"
                                        allowfullscreen
                                        loading="lazy"></iframe>
                                    {% endif %}
                                  {% else %}
                                    {% if block.settings.image != blank %}
                                      <img
                                        src="{{ block.settings.image | img_url: 'master' }}"
                                        alt="{{block.settings.title }}"
                                        class="lazyload">
                                    {% endif %}
                                  {% endif %}


                                  <div class="content">
                                    <p class="title">{{ block.settings.title }}</p>
                                    <span class="link--primary">
                                      {{- block.settings.linkText -}}
                                    </span>
                                  </div>
                                </a>
                              </li>
                            {% endif %}
                          {% endif %}
                      {% endcase %}
                    {%- endfor %}
                  </ul>
                </div>
              </div>
            </div>
          </div>
        {% else %}
          {% comment %} SUB NAV {% endcomment %}
          {% if link.levels == 1 %}
            <div class="sub-nav accordion-content">
              <div class="flex">
                <ul class="sub-nav-links">
                  {% for childlink in link.links %}
                    <li>
                      <a
                        href="{{ childlink.url }}"
                        title="{{ childlink.title | escape }}"
                        aria-controls="MenuChildren-{{ parent_index }}-{{ child_index }}"
                        data-meganav-type="parent"
                        {% unless request.page_type == 'index' %}
                        {%- if childlink.active %}
                        aria-current="page"
                        {% endif -%}
                        {% endunless %}>
                        {{ childlink.title | escape -}}
                      </a>
                    </li>
                  {% endfor %}
                </ul>
              </div>
            </div>
          {% endif %}
        {% endif %}
      </li>
    {% else %}
      <!-- If NO child menus are there -->


      {% assign linkTitle = link.title | downcase | remove: ' ' %}


      <li class=" {% if linkTitle contains 'getstarted' %}has-big-nav{%else%} {% if linkTitle contains 'support' %} has-sub-nav{%endif%}{%endif%}">
        <a
          href=" {{ link.url}}"
          title="{{ link.title | escape }}"
          class="{% if linkTitle contains 'support' %}site-nav__link none-980{%endif%}">
          {{ link.title | escape }}
        </a>
        {% if linkTitle contains 'support' %}
          <p class="accordion-toggle block-980">{{ link.title | escape }}</p>
        {% endif %}


        {% if linkTitle contains 'getstarted' %}
          <div class="big-nav none-980">
            <div class="container-1360">
              <div class="flex center">
                <div class="two-column-product-section none-980">
                  <ul class="flex-wrap">
                    {% for block in section.blocks -%}
                      {%- case block.type -%}
                        {%- when 'block' -%}
                          {% assign linkTitlecat = block.settings.category | downcase | remove: ' ' %}
                          {% if linkTitle contains linkTitlecat %}
                            {% if block.settings.title != blank %}
                              <li class="
                                  featured-card {% if block.settings.content_alignment == "center" %} center-align{%endif%} {% if block.settings.content_alignment == "right" %} right-align{%endif%}{% if
                                  block.settings.content_alignment == "left" %} left-align{%endif%}
                                ">
                                <a
                                  href="{{block.settings.link}}"
                                  title="{{block.settings.linkText}}"
                                  class="inner-wrapper">
                                  <img
                                    src="{{ block.settings.image | img_url: 'master' }}"
                                    alt="{{block.settings.title }}"
                                    class="lazyload">
                                  <div class="content">
                                    <p class="title">{{ block.settings.title }}</p>
                                    <span class="link--primary">
                                      {{- block.settings.linkText -}}
                                    </span>
                                  </div>
                                </a>
                              </li>
                            {% endif %}
                          {% endif %}
                      {% endcase %}
                    {%- endfor %}
                  </ul>
                </div>
              </div>
            </div>
          </div>
        {% endif %}
        {% if linkTitle contains 'support' %}
          <div class="sub-nav accordion-content">
            <div class="flex">
              <ul class="sub-nav-links">
                <div class="contact-into-section">
                  <ul>
                    {% if settings.chatText != blank %}
                      <li>
                        <span class="title">Chat:</span>
                        <a href="{{settings.chatLink}}" title="{{settings.chatText }}">{{ settings.chatText }}</a>
                      </li>
                    {% endif %}
                    {% if settings.number != blank %}
                      <li>
                        <span class="title">Phone:</span>
                        <a href="tel:{% if settings.numberPlusSign == true %}+{%endif%}{{settings.number}}" title="call-{{settings.number}}">
                          {{- settings.number -}}
                        </a>
                      </li>
                    {% endif %}
                    {% if settings.textnumber != blank %}
                      <li>
                        <span class="title">Text Us:</span>
                        <a href="{{settings.textnumber}}" title="text-{{settings.textnumber}}">
                          {{- settings.textnumber -}}
                        </a>
                      </li>
                    {% endif %}
                    {% if settings.emailAddress != blank %}
                      <li>
                        <span class="title">Email:</span>
                        <a href="mailto:{{settings.emailAddress}}" title="mail-{{settings.emailAddress}}">
                          {{ settings.emailAddress -}}
                        </a>
                      </li>
                    {% endif %}
                    {% if settings.trackLinkText != blank and settings.trackLink != bank %}
                      <li>
                        <span class="title">Orders:</span>
                        <a href="{{settings.trackLink }}" title="{{settings.trackLinkText }}">
                          {{- settings.trackLinkText -}}
                        </a>
                      </li>
                    {% endif %}
                  </ul>
                </div>
              </ul>
            </div>
          </div>
        {% endif %}
      </li>
    {% endif %}
  {% endfor %}
</ul>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    const menuItems = document.querySelectorAll('.has-big-nav');

    menuItems.forEach(item => {
      item.addEventListener('mouseenter', function() {
        const videos = this.querySelectorAll('video');
        videos.forEach(video => {
          video.currentTime = 0;
          video.play();
        });
      });
    });
  });
</script>